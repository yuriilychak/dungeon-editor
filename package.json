{
  "name": "dungeon-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@material-ui/utils": "^3.0.0-alpha.3",
    "@types/jszip": "^3.1.5",
    "file-saver": "^2.0.1",
    "i18next": "^15.0.9",
    "jszip": "^3.2.1",
    "npm": "^6.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.6.1",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext .jsx --ext .js src/",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzumeSetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/Theme.js",
      "!<rootDir>/src/**/constant/*.js",
      "!<rootDir>/src/**/container/*.js",
      "!<rootDir>/src/**/enum/*.js",
      "!<rootDir>/src/**/state/*.js"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "author": "Yurii Lychak",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^24.7.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "react-test-renderer": "^16.8.6"
  }
}
