{
  "name": "dungeon-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@material-ui/utils": "^3.0.0-alpha.3",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "downshift": "^3.4.8",
    "file-dialog": "0.0.7",
    "file-saver": "^2.0.2",
    "i18next": "^15.1.3",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "npm": "^6.14.5",
    "prop-types": "^15.7.2",
    "rc-color-picker": "^1.2.6",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^11.0.5",
    "react-dnd": "^7.7.0",
    "react-dnd-html5-backend": "^7.7.0",
    "react-dom": "^16.13.1",
    "react-drag-and-drop": "^3.0.0",
    "react-dropzone": "^10.2.2",
    "react-i18next": "^10.13.2",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "react-select": "^3.1.0",
    "react-sortable-tree": "^2.7.1",
    "react-sortable-tree-theme-file-explorer": "^2.0.0",
    "redux": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext .jsx --ext .js src/",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "test-update-snapshots": "jest -u --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzumeSetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/Theme.js",
      "!<rootDir>/src/**/index.js",
      "!<rootDir>/src/**/types.js",
      "!<rootDir>/src/**/constant/*.js",
      "!<rootDir>/src/**/enum/*.js",
      "!<rootDir>/src/**/state.js"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "author": "Yurii Lychak",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "react-test-renderer": "^16.13.1"
  }
}
